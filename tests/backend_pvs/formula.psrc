module A {
    formula foo(int a, b: int) =
        a + b;

    formula factorial(int a: int) =
        a != 1 ? factorial(a - 1) : 1
        measure a;

    formula gcd(int a, b: int) =
        a < b ? gcd(a, b - a) :
            (a > b ? gcd(a - b, b) : a)
        measure a + b;

    lemma foo() = 0 ? true : false;
}
