formula g(nat a, b: bool) = a = 12 & g(1, 2);
formula f(nat a, b: bool) = a < 1 & b < 2 & f(1, 2);

bar(nat a: nat b, c)
pre a > 0
{
    b = 0 || c = 0
}
post f(b, c) & b = 0;
