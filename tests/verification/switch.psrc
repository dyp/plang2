/*

<test>
    = forall nat b, nat a. b = 0 & (a = 7 or a >= 0 or a <= 3) & P_bar(a) => Q_bar(a, b)
    = forall nat b, nat a. (a = 7 or a >= 0 or a <= 3) & P_bar(a) => (exists . b = 0)
    = forall nat b, nat a. (!(a <= 3)) & b = 1 & (!(a = 7)) & P_bar(a) & (a = 8 or a >= 4 or a <= 5) & (!(a >= 0)) => Q_bar(a, b)
    = forall nat b, nat a. (!(a <= 3)) & (!(a = 7)) & P_bar(a) & (a = 8 or a >= 4 or a <= 5) & (!(a >= 0)) => (exists . b = 1)
    = forall nat b, nat a. (!(a <= 3)) & (!(a = 8)) & (!(a <= 5)) & (!(a = 7)) & P_bar(a) & (!(a >= 4)) & (!(a >= 0)) & b = 2 => Q_bar(a, b)
    = forall nat b, nat a. (!(a <= 3)) & (!(a = 8)) & (!(a <= 5)) & (!(a = 7)) & P_bar(a) & (!(a >= 4)) & (!(a >= 0)) => (exists . b = 2)
</test>

*/

bar(nat a: nat b)
pre a > 0
{
    switch (a) {
        case 0..3, 7: b = 0
        case 4..5, 8: b = 1
        default: b = 2
    }
}
post b >= 0 & b <= 2;
